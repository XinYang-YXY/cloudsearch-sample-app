{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
        {
            "name": "cloudsearch-backend",
            "image": "102153735617.dkr.ecr.ap-southeast-1.amazonaws.com/cloudsearch-sample-backend:multi",
            "hostname": "cloudsearch-backend",
            "essential": true,
            "memory": 256
        },
        {
            "name": "cloudsearch-frontend",
            "image": "102153735617.dkr.ecr.ap-southeast-1.amazonaws.com/cloudsearch-sample-frontend:multi",
            "hostname": "cloudsearch-frontend",
            "essential": true,
            "memory": 256
        },
        {
            "name": "cloudsearch-router",
            "image": "102153735617.dkr.ecr.ap-southeast-1.amazonaws.com/cloudsearch-sample-router:multi",
            "hostname": "cloudsearch-router",
            "essential": true,
            "portMappings": [
                {
                    "hostPort": 80,
                    "containerPort": 80
                }
            ],
            "links": ["cloudsearch-backend", "cloudsearch-frontend"],
            "memory": 128
        }
    ]
}